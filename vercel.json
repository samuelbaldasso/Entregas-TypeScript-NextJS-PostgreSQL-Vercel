{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "routes": [
    {
      "/api/data.json": {
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "content-type": "application/json"
        }
      }
    }
  ]
}
